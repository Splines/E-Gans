                        93     SnakeLengthener:
                        94     ; Storage: starting from 28h
0043 7928               95     mov R1, #28h
                        96     ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                        97     ;;;;;;;;;;;;;;;;;;;;;; Snake Follow Loop ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                        98     ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                        99     SnakeFollowLoop:
                       100     ; R1 is our incrementor
                       101     ; Given Buffer1 filled
                       102     ; Save old Head to Buffer2
                       103
0045 8722              104     mov Buffer2X, @R1
0047 09                105     inc R1
0048 8723              106     mov Buffer2Y, @R1
                       107
                       108     ;;;;;;;;;;;;;;;;;;;;; Buffer1 X/Y ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                       109     ; Move Buffer1 to Head (Update first element with Buffer1)
                       110     ; mov A, Buffer1X ; OR: Buffer1X, SnakeLedRepresentationX -> Save in SnakeLedRepresentationX
                       111     ; orl SnakeLedRepresentationX, A
004A 19                112     dec R1
004B A720              113     mov @R1, Buffer1X
                       114
                       115     ; mov A, Buffer1Y ; OR: Buffer1Y, SnakeLedRepresentationY -> save in SnakeLedRepresentationY
                       116     ; orl SnakeLedRepresentationY, A
004D 09                117     inc R1
004E A721              118     mov @R1, Buffer1Y
0050 09                119     inc R1
                       120
                       121     ; Update snake with pos from buffer 1
0051 852180            122     mov P0, Buffer1Y
0054 852090            123     mov P1, Buffer1X
                       124
                       125
                       126     ;;;;;;;;;;;;;;;;;;;;;;;;;; Break? ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                       127     ; Check if next element is 0
0057 E7                128     mov A, @R1
0058 6029              129     jz endUpdateSnakeFromBreak
                       130
                       131     ;;;;;;;;;;;;;;;;;;;;; Buffer2 X/Y ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                       132     ; Store second element in Buffer1
005A 8720              133     mov Buffer1X, @R1
005C 09                134     inc R1
005D 8721              135     mov Buffer1Y, @R1
                       136
                       137     ; Move buffer2 to second element
005F 19                138     dec R1
0060 A722              139     mov @R1, Buffer2X
